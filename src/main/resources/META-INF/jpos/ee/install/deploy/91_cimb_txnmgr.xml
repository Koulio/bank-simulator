<txnmgr name="txnmgr" logger="Q2" class="org.jpos.transaction.TransactionManager">
    <property name="space" value="tspace:default"/>
    <property name="queue" value="CimbQueue"/>
    <property name="max-sessions" value="100"/>
    <property name="debug" value="true"/>

    <participant class="org.jpos.transaction.Debug" logger="Q2"/>

    <participant class="com.midtrans.bank.transaction.SelectTransaction" logger="Q2">
        <property name="0200 000000" value="CardCheck Sale"/>
        <property name="0800 990000" value="Echo"/>
    </participant>

    <group name="Sale">
        <participant class="com.midtrans.bank.transaction.cimb.Sale" logger="Q2"/>
    </group>

    <group name="Echo">
        <participant class="com.midtrans.bank.transaction.cimb.Echo" logger="Q2"/>
    </group>

    <group name="CardCheck">
        <participant class="com.midtrans.bank.transaction.CardCheck" logger="Q2">
            <property name="r00" value="4111111111111111"/>
            <property name="r05" value="4545454545454545"/>
        </participant>
    </group>
</txnmgr>
