<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.midtrans.bank.core.model.BankISOResponse" table="bank_iso_response" catalog="peacock">
        <id name="id" type="java.lang.Long" column="pk_bank_iso_response">
            <generator class="identity" />
        </id>
        <property name="description" type="string" column="description"/>
        <property name="fieldRequestNo" type="java.lang.Integer" column="field_request_no"/>
        <property name="fieldResponseNo" type="java.lang.Integer" column="field_response_no"/>
        <property name="staticValue" type="string" column="static_value"/>
        <property name="valueResponseType" type="java.lang.Integer" column="value_response_type"/>

        <many-to-one name="commandTypeCondition" class="com.midtrans.bank.core.model.CommandTypeCondition" fetch="join">
            <column name="fk_comm_type_cond"/>
        </many-to-one>
    </class>
</hibernate-mapping>