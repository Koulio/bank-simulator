<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.midtrans.bank.core.model.CommandTypeCondition" table="command_type_condition" catalog="peacock">
        <id name="id" type="java.lang.Long" column="pk_comm_type_cond">
            <generator class="identity" />
        </id>
        <property name="active" type="boolean">
            <column name="active" sql-type="TINYINT(1)" />
        </property>
        <property name="note" type="string" column="note"/>

        <many-to-one name="bankTransConfiguration" class="com.midtrans.bank.core.model.BankTransConfiguration" fetch="join">
            <column name="fk_bank_trans_conf"/>
        </many-to-one>

        <many-to-one name="lookupOfConditionType" class="com.midtrans.bank.core.model.Lookup" fetch="join">
            <column name="fk_lookup_condition_type"/>
        </many-to-one>
    </class>
</hibernate-mapping>