<txnmgr name="txnmgr" logger="Q2" class="org.jpos.transaction.TransactionManager">
    <property name="space" value="tspace:default"/>
    <property name="queue" value="TxnQueue"/>
    <property name="max-sessions" value="100"/>
    <property name="debug" value="true"/>

    <participant class="org.jpos.transaction.Debug" logger="Q2"/>
    <participant class="com.midtrans.bank.logic.transaction.StartTransaction" logger="Q2"/>
    <participant class="com.midtrans.bank.logic.transaction.FindBankTransConfiguration" logger="Q2"/>
    <participant class="com.midtrans.bank.logic.transaction.SelectCommand" logger="Q2">
        <property name="Echo" value="DoEcho EchoResponse"/>
        <property name="Sale" value="FindTerminal CheckTrace CreateTrace SaveTrace CreateTransaction DoSale UpdateTerminal SaveTerminal BuildResponse SaveTransaction"/>
        <property name="Void" value="FindTerminal CheckTrace CreateTrace SaveTrace FindTransaction CheckBalance CreateVoidTxn DoVoid UpdateTerminal SaveTerminal BuildResponse SaveVoidTxn SaveTransaction"/>
        <property name="ReversalSale" value="FindTerminal FindTrace FindTransaction DoReversalSale UpdateTerminal SaveTerminal BuildResponse SaveTransaction"/>
        <property name="ReversalVoid" value="FindTerminal FindTrace FindVoidTxn FindTransaction DoReversalVoid UpdateTerminal SaveTerminal BuildResponse SaveVoidTxn SaveTransaction"/>
        <property name="Settlement" value="FindTerminal CheckTrace CreateTrace SaveTrace CreateSettlementTxn DoSettlement ResetTerminal ResetBatchTerminal SaveTerminal BuildResponse SaveSettlementTxn DeactivateTransactions"/>
        <property name="BatchUpload" value="FindTerminal CheckTrace CheckBatchNumber CreateTrace SaveTrace FindTransaction CreateBatchTxn DoBatchUpload UpdateBatchTerminal SaveTerminal BuildResponse SaveBatchTxn"/>
        <property name="SettlementTrailer" value="FindTerminal CheckTrace CreateTrace SaveTrace CreateSettlementTxn DoSettlementTrailer ResetTerminal ResetBatchTerminal SaveTerminal BuildResponse SaveSettlementTxn DeactivateTransactions"/>
    </participant>
    <participant class="com.midtrans.bank.logic.transaction.EndTransaction" logger="Q2"/>

    <group name="FindTerminal">
        <participant class="com.midtrans.bank.logic.transaction.FindTerminal" logger="Q2"/>
